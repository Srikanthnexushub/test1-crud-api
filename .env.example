# ===========================================
# ENVIRONMENT VARIABLES TEMPLATE
# ===========================================
# Copy this file to .env and fill in your actual values
# NEVER commit .env to version control!

# Database Configuration
DATABASE_URL=jdbc:postgresql://localhost:5433/Crud_db
DATABASE_USERNAME=postgres
DATABASE_PASSWORD=your_secure_database_password_here

# JWT Security Configuration
# IMPORTANT: Generate a secure 256-bit secret key
# Example: openssl rand -base64 32
JWT_SECRET=your_secure_jwt_secret_key_at_least_32_characters_long

# Optional: Override default settings
# SERVER_PORT=8080
# SPRING_PROFILES_ACTIVE=dev
